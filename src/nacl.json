[
    {
      "group": "admin",
      "permissions": [
        {
          "resource": "*",
          "methods": "*",
          "action": "allow"
        }
      ]
      
    },
    {
      "group": "user",
      "permissions": [
        {
          "resource": "/products",
          "methods": ["POST", "GET", "PUT"],
          "action": "allow"
        },
        {
          "resource": "/users/*",
          "methods": ["DELETE","PUT"],
          "action": "allow"
        }
      ]
    }
]